{
  "expo": {
    "name": "FryDay",
    "slug": "FryDay",
    "scheme": "fryday",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fryday-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/fryday-splash-logo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF5B22"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hamsj2413.fryday"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fryday-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bmbx08.FryDay"
    },
    "web": {
      "favicon": "./assets/fryday-favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-build-properties",
      "expo-secure-store",
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "timePicker": {
              "headerBackground": {"light": "#FF5B22", "dark": "#555555"},
              "numbersSelectorColor": {"light": "#FF5B22", "dark": "#80CBC4"},
              "background": {"light": "#FFFFFF", "dark": "#383838"},
              "numbersBackgroundColor": {"light": "#EEEEEE", "dark": "#555555"}
            }
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0585f789-8134-49ad-b995-6b872f05c64c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0585f789-8134-49ad-b995-6b872f05c64c"
    }
  }
}

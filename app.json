{
  "expo": {
    "name": "FryDay",
    "slug": "FryDay",
    "scheme": "fryday",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fryday-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/fryday-splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF5B22"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.crispycrew.fryday.qa",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "naversearchapp",
          "naversearchthirdlogin"
        ]
      },
      "appleTeamId": "4378PR9BBA",
      "buildNumber": "8"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fryday-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bmbx08.FryDay",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/fryday-favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "timePicker": {
              "headerBackground": {
                "light": "#FF5B22",
                "dark": "#555555"
              },
              "numbersSelectorColor": {
                "light": "#FF5B22",
                "dark": "#80CBC4"
              },
              "background": {
                "light": "#FFFFFF",
                "dark": "#383838"
              },
              "numbersBackgroundColor": {
                "light": "#EEEEEE",
                "dark": "#555555"
              }
            }
          }
        }
      ],
      [
        "@react-native-seoul/kakao-login",
        {
          "kakaoAppKey": "c2eaa0f8dae4116f5ee998144c4aa122",
          "overrideKakaoSDKVersion": "2.11.2",
          "kotlinVersion": "2.1.20"
        }
      ],
      [
        "@react-native-seoul/naver-login",
        {
          "urlScheme": "frydaynaver"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useModularHeaders": true,
            "extraPods": [
              {
                "name": "GoogleUtilities",
                "modular_headers": true
              },
              {
                "name": "GoogleDataTransport",
                "modular_headers": true
              },
              {
                "name": "nanopb",
                "modular_headers": true
              },
              {
                "name": "FirebaseCoreInternal",
                "modular_headers": true
              }
            ]
          },
          "android": {
            "extraMavenRepos": [
              "https://devrepo.kakao.com/nexus/content/groups/public/"
            ]
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "@react-native-firebase/crashlytics"
    ],
    "extra": {
      "eas": {
        "projectId": "0585f789-8134-49ad-b995-6b872f05c64c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0585f789-8134-49ad-b995-6b872f05c64c"
    },
    "owner": "frydayapp"
  }
}
